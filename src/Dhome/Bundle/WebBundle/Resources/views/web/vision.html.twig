{% extends 'DhomeWebBundle:web:layout.html.twig' %}

{% block content %}

    {% block breadcrumb %}
        {% include 'DhomeWebBundle:web/partials:_breadcrumb.html.twig' %}
    {% endblock %}

    <div class="projects">
        <div class="project__header">OUR INSPIRATIONS</div>

        <div class="card-columns">

            {% for i in 0..3 %}
            <div class="card">
                <a href="">
                    <img class="card-img-top img-fluid img-lazy" src="{{ asset('ui/web/img/b.jpg') }}" alt="Card image cap">
                </a>
                <div class="card-block">
                    <h4 class="card-title"><a href="#">Card title that wraps to a new line</a></h4>
                    <p class="card-text"><a href="#">This is a longer card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</a></p>
                    <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
                </div>
            </div>

            <div class="card">
                <a href="">
                    <img class="card-img-top img-fluid img-lazy" src="{{ asset('ui/web/img/h.jpg') }}" alt="Card image cap">
                </a>
                <div class="card-block">
                    <h4 class="card-title"><a href="#">Card title that wraps to a new line</a></h4>
                    <p class="card-text"><a href="#">This is a longer card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</a></p>
                    <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
                </div>
            </div>

            <div class="card">
                <a href="">
                    <img class="card-img-top img-fluid img-lazy" src="{{ asset('ui/web/img/a.jpg') }}" alt="Card image cap">
                </a>
                <div class="card-block">
                    <h4 class="card-title"><a href="#">Card title that wraps to a new line</a></h4>
                    <p class="card-text"><a href="#">This is a longer card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</a></p>
                    <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
                </div>
            </div>
            {% endfor %}

        </div>
    </div>

    {% block paginator %}
        {% include 'DhomeWebBundle:web/partials:_paginator.html.twig' %}
    {% endblock %}

{% endblock %}
