<!DOCTYPE html>
<html xmlns:og="http://opengraphprotocol.org/schema/" lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="distribution" content="global">
        <meta name="robots" content="all">

        <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
        <link rel="icon" href="/favicon.ico" type="image/x-icon">
        <link rel="apple-touch-icon" sizes="120x120" href="/apple-touch-icon.png">

        {% block head %}
            <title>DDHOMETH : home and decor</title>
            <link rel="image_src" href="{{ asset('ui/web/img/dhome-logo.png') }}">
            <meta name="keywords" content="ddhome, ddhometh, ddsign at home, design at home, modern, design, home building, สร้างบ้าน, ตกแต่งบ้าน, interiror, exterior">
            <meta name="description" content="ddhome, ddhometh, ddsign at home, design at home, modern, design, home building, สร้างบ้าน, ตกแต่งบ้าน, interiror, exterior">

            <meta property="fb:app_id" content="314704065533212">
            <meta property="og:title" content="DDHOMETH - home and decor">
            <meta property="og:type" content="object">
            <meta property="og:url" content="http://www.ddhometh.com">
            <meta property="og:image" content="{{ asset('ui/web/img/dhome-logo.png') }}">
            <meta property="og:description" content="ddhome, ddhometh, ddsign at home, design at home, modern, design, home building, สร้างบ้าน, ตกแต่งบ้าน, interiror, exterior">

            <meta name="twitter:card" content="summary_large_image">
            <meta name="twitter:title" content="DDHOMETH">
            <meta name="twitter:description" content="ddhome, ddhometh, ddsign at home, design at home, modern, design, home building, สร้างบ้าน, ตกแต่งบ้าน, interiror, exterior">
            <meta name="twitter:image" content="{{ asset('ui/web/img/dhome-logo.png') }}">
        {% endblock %}

        {% block stylesheets %}
            <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600" rel=stylesheet>
            <link rel="stylesheet" href="{{ asset('assets/web/css/style.css') }}" type="text/css"/>
        {% endblock %}

        <script type="text/javascript" async="" src="http://www.google-analytics.com/ga.js"></script>

    </head>

    <body>

    {% block navbar %}
        {% include 'DhomeWebBundle:web/partials:_navbar.html.twig' %}
    {% endblock %}

    {% block carousel %}{% endblock %}

    {% block body %}{% endblock %}

    <div class="container main-content">
        {% block content %}{% endblock %}
    </div>

    {% block footer %}
        {% include 'DhomeWebBundle:web/partials:_footer.html.twig' %}
    {% endblock %}

    {% block ui_script %}
        <script>
            window.fbAsyncInit = function() {
                FB.init({
                    appId      : '314704065533212',
                    xfbml      : true,
                    version    : 'v2.7'
                });
            };

            (function(d, s, id){
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) {return;}
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_US/sdk.js";
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        </script>
    {% endblock %}

    {% block javascripts %}
        <script src="{{ asset('assets/web/js/app.js') }}"></script>
    {% endblock javascripts %}
    </body>
</html>
